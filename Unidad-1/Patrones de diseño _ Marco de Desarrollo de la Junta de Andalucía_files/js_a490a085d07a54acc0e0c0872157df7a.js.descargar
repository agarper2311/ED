// $Id$

// Modifica las URLs de autocompletado AJAX cuando se accede desde HTTPS.
Drupal.behaviors.autocompleteFix = function (context) {
  $('input.autocomplete:not(.autocomplete-processed)', context).each(function () {
    this.value = this.value.replace(/^http(s)?:/, document.location.protocol);
  });
};

// Focus en el formulario de login
Drupal.behaviors.loginFocus = function (context) {
  $('#user-login input#edit-name', context).focus();
};

// Sólo permite seleccionar dos checkboxes en la página revisiones
Drupal.behaviors.revisionsDiff = function () {
  var $checkbox = $('#revisioning-revisions-summary table.table-revisions input.form-checkbox');
  $checkbox.click(function() {
    if ($('#revisioning-revisions-summary table.table-revisions input.form-checkbox:checked').length > 2) {
      $checkbox.removeAttr('checked');
      $(this).attr('checked', true);
    }
  });
};

// Comportamientos específicos del portal MADEJA.
Drupal.behaviors.madejaPortal = function () {
  var $element = $('body:not(.section-search) #edit-search-theme-form-1');
  var $submit = $('body:not(.section-search) #edit-submit');
  var value = Drupal.t('Búsqueda...');

  // Add focus/blur label behavior to search box.
  $element.bind('focus', function () {
    if ($element.val() === value) {
      $element.val('').addClass('has-value');
    }
  });
  $element.bind('blur', function () {
    if ($element.val() === '' || $element.val() === value) {
      $element.val(value).removeClass('has-value');
    }
    else {
      $element.addClass('has-value');
    }
  });
  $element.trigger('blur');
  
  $submit.click(function () {
    if ($element.val() === value) {
      $element.val('');
    }
  });
};

// Modifica los botones hierarchical_select para pasarlos de type="submit" a type="button".
Drupal.behaviors.hierarchicalSelectFix = function () {
  $('.hierarchical-select .add-to-dropbox').each(function () {
    var id = $(this).attr('id');
    $(this).parent().append(Drupal.theme('hierarchicalSelectButton', id));
    $(this).remove();
  });
};

Drupal.theme.prototype.hierarchicalSelectButton = function (id) {
  return '&nbsp' + '<input id="' + id + '" type="button" class="form-submit add-to-dropbox" value="' + Drupal.t('Add') + '" name="op" />';
};

//Habilita/Deshabilita el botón de ejecutar operación múltiple 
Drupal.behaviors.madejaVBODisableExecuteButton = function () {
  // Por defecto se deshabilita el botón si no hay ningún elemento seleccionado
  var i = $('table td input:checked').length;
  if (i < 1) {
    $('div#views-bulk-operations-select input').attr('disabled', true);
  } else {
    $('div#views-bulk-operations-select input').removeAttr('disabled');
  }

  $('tr.rowclick input').bind('click', function() {
    var n = $('table td input:checked').length;
    if (n < 1) {
      $('div#views-bulk-operations-select input').attr('disabled', true);
    } else {
      $('div#views-bulk-operations-select input').removeAttr('disabled');
    }
  });

  $('th.select-all').bind('click', function() {
    var n = $('th.select-all input:checked').length;
    if (n < 1) {
      $('div#views-bulk-operations-select input').attr('disabled', true);
    } else {
      $('div#views-bulk-operations-select input').removeAttr('disabled');
    }
  });
};

//Oculta las pestañas en las ventanas de confirmación de formulario, etc
Drupal.behaviors.madejaWorkspaceHiddenTabs = function () {
  if ($('div.view form.confirmation').length > 0) {
    $('#tabs-wrapper ul.tabs').hide();
    $('div.help').hide();
  }
  if ($('div.view input#edit-execute').length > 0) {
    $('#tabs-wrapper ul.tabs').hide();
    $('div.help').hide();
  }
}

